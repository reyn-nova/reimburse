generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id    Int     @id @default(autoincrement())
  name  String?
  notes Note[]
}

model Note {
  id        Int      @id @default(autoincrement())
  value     String?
  projectId Int?
  project   Project? @relation(fields: [projectId], references: [id])
}
